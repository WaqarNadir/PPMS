<?xml version="1.0" encoding="utf-8"?>
<Application
  Name="LightSwitchApplication.PPMS.DesktopClient" xmlns="http://schemas.microsoft.com/LightSwitch/2010/xaml/model">
  <Application.Attributes>
    <Partial />
  </Application.Attributes>
  <EntityType>
    <EntityType.Attributes>
      <Proxy
        Target="ServiceInterface:Customer" />
      <DefaultScreen
        Value="VIEW_CustomerDetail">
        <DefaultScreen.Attributes>
          <ScreenParameterMapping
            KeyProperty="Customer/Properties[Customer_ID]"
            ScreenParameter="VIEW_CustomerDetail/Properties[CustomerCustomer_ID]" />
        </DefaultScreen.Attributes>
      </DefaultScreen>
    </EntityType.Attributes>
  </EntityType>
</Application>